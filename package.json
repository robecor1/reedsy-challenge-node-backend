  },
  "homepage": "https://github.com/robecor1/reedsy-challenge-node-backend#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/swagger-ui-express": "^4.1.3",
    "artillery": "^2.0.0-33",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
